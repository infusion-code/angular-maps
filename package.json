  "license": "MIT",
  "dependencies": {
    "@angular/common": ">=2.4.9",
    "@angular/compiler": ">=2.4.9",
    "@angular/core": ">=2.4.9",
    "bingmaps": "^1.0.15",
    "font-awesome": "^4.6.3",
    "json-loader": "^0.5.4",
    "rxjs": "^5.2.0",
    "zone.js": "^0.8.2"
  },
  "devDependencies": {
    "@angular/animations": ">=2.4.9",
    "@angular/platform-browser": ">=2.4.9",
    "@angular/platform-server": ">=2.4.9",
    "@angular/compiler-cli": ">=2.4.9",
    "codelyzer": ">=3.0.1",
    "tslint": ">=5.4.2",
    "@types/node": "^7.0.5",
    "typescript": "^2.3.2"
  }
}
